BaseType_t ThreadSafeWrapper_QueueInit( UBaseType_t xQueueSize, size_t uxItemSize, QueueHandle_t ThreadSafeQueue )
{
    BaseType_t xReturn = pdPASS;

	ThreadSafeQueue = xQueueCreate( xQueueSize, uxItemSize );

	return xReturn;
}

void ThreadSafeWrapper_QueueSend( void )
{
	NonThreadSafeAPI()
}
